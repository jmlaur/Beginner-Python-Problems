def addTwoNumbers(l1, l2):
    string = ""
    string2 = ""
    l1.reverse()
    l2.reverse()
    for i in l1:
        string += str(i)
    for i in l2:
        string2 += str(i)
    result = int(string) + int(string2)
    output = list(str(result))
    output.reverse()
    return output


l1 = [2,4,3]
l2 = [5,6,4]
print(addTwoNumbers(l1, l2))
